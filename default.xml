<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="revel"
           fetch="https://github.com/revel/"
           revision="master"/>

  <default revision="refs/heads/master"
           remote="revel"/>

  <project name="revel"   remote="revel" path="src/github.com/revel/revel"    groups="core,all" />
  <project name="modules" remote="revel" path="src/github.com/revel/modules"  groups="core,all" />
  <project name="cmd"     remote="revel" path="src/github.com/revel/cmd"      groups="core,all" />
  <project name="cron"    remote="revel" path="src/github.com/revel/cron"     groups="core,all" />

	<!-- optional -->
  <project name="heroku-buildpack-go-revel"  remote="revel"
           path="src/github.com/revel/heroku-buildpack-go-revel"    groups="optional,all,notdefault" />

  <project name="revel.github.io"  remote="revel"
           path="src/github.com/revel/revel.github.io"              groups="docs,all,notdefault" />
  <project name="samples"  remote="revel" path="src/github.com/revel/samples"   groups="docs,all,notdefault" />
  <project name="rfcs"     remote="revel" path="src/github.com/revel/rfcs"      groups="docs,all,notdefault" />

</manifest>
